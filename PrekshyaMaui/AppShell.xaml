<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PrekshyaMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PrekshyaMaui"
    Shell.FlyoutBehavior="Flyout"
    xmlns:views="clr-namespace:PrekshyaMaui.Views"
    Title="PrekshyaMaui">

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <FlyoutItem Title="Graph page">
        <ShellContent
        ContentTemplate="{DataTemplate views:BarGraphPage}"
        Route="BarGraphPage" />
    </FlyoutItem>
    <FlyoutItem Title="Pie page">
        <ShellContent
     ContentTemplate="{DataTemplate views:PieChartPage}"
     Route="PieChartPage" />
    </FlyoutItem>
    <FlyoutItem Title="LineGraph page">
        <ShellContent
   ContentTemplate="{DataTemplate views:LineGraphPage}"
   Route="LineGraphPage" />
    </FlyoutItem>
    <FlyoutItem Title="CustomGraph page">
        <ShellContent
 ContentTemplate="{DataTemplate views:CustomGraphPage}"
 Route="CustomGraphPage" />
    </FlyoutItem>
</Shell>
